{
  "version": 2,
  "builds": [
    { "src": "examples/http-server/*.js","use": "@vercel/static" },
    { "src": "lib/**","use": "@vercel/static" },
    { "src": "*.json","use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/","dest": "examples/http-server/index.html" },
    { "src": "/(.*)","dest": "examples/http-server/index.js" }
  ]
}
